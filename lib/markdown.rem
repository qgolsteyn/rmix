(def markdown (@ 
    (def h1 (@ # (#) "\n\n"))
    (def h2 (@ ## (#) "\n\n"))
    (def h3 (@ ### (#) "\n\n"))
    (def h4 (@ #### (#) "\n\n"))
    (def h5 (@ ##### (#) "\n\n"))
    (def h6 (@ ###### (#) "\n\n"))
    
    (def p (@ (#) "\n\n"))
    
    (def ul (@
        (def item (@ "-" (#) "\n"))
        (#) "\n"))
    
    (def ol (@
        (def item (@ "1." (#) "\n"))
        (#) "\n"))

    (write example.md (string/join (#)))))
