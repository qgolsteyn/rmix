(; Mapping tests in order to simplify output when all tests pass in a test suite)
(defn.post tests
    (def.post testSuiteName (list.head (#)))
    (defn.post test (?
        (list.get 1 (#))
        (test.success (testSuiteName) (list.get 0 (#)))
        (test.failure (testSuiteName) (list.get 0 (#)))))
        (list.tail (#!)))
