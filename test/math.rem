(tests (name math mapping)
        (test (name it can add two numbers) (node.compare (+ 1 1) 2))
        (test (name it can subtract two numbers) (node.compare (- 1 1) 0))
        (test (name it can multiply two numbers) (node.compare (* 10 2) 20))
        (test (name it can divide two numbers) (node.compare (/ 10 2) 5))
        (test (name it can calculate the power of two numbers) (node.compare (** 2 2 2) 16))
        (test (name it returns the absolute value of a list of numbers) (node.compare (root (math.abs -1 -2 -3 4 0)) (root 1 2 3 4 0)))
        (test (name it can perform complex arithmetic) (node.compare (* 2 (- 10 (/ 10 2))) 10)))
