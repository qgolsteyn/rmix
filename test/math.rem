(.tests "math mapping"
        (.test "it can add two numbers" (.compare (+ 1 1) 2))
        (.test "it can subtract two numbers" (.compare (- 1 1) 0))
        (.test "it can multiply two numbers" (.compare (* 10 2) 20))
        (.test "it can divide two numbers" (.compare (/ 10 2) 5))
        (.test "it can perform complex arithmetic" (.compare (* 2 (- 10 (/ 10 2))) 10)))